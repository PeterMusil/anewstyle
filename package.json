{
  "name": "anew-style-npm-config",
  "version": "2.0.1",
  "description": "Updated on 13.1.2024",
  "private": true,
  "license": "ISC",
  "author": "Lukáš Chylík",
  "type": "module",
  "scripts": {
    "start": "concurrently \"astro dev\" \"bun run stylelint\" \"bun run scss:watch\"",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "scss:build": "sass src/styles/style.scss | postcss -o public/css/style.css",
    "scss:watch": "nodemon -e scss -x bun run scss:build",
    "stylelint": "stylelint --fix src/**/*.scss --custom-syntax postcss-scss"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/sitemap": "^3.6.0",
    "@fancyapps/ui": "^5.0.36",
    "@google/model-viewer": "^4.1.0",
    "astro": "^5.16.6",
    "depcheck": "^1.4.7",
    "three": "^0.172.0",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "cssnano": "^7.1.2",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "postcss-preset-env": "^10.5.0",
    "postcss-scss": "^4.0.9",
    "sass": "^1.97.0",
    "stylelint": "^16.26.1",
    "stylelint-order": "^7.0.0"
  }
}